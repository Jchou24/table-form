{
  "name": "jctk-table-form",
  "version": "0.1.9",
  "main": "dist/jctk-table-form.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib ./src/index.js",
    "build-demo-dev": "vue-cli-service build --mode development --target app --dest demo",
    "build-demo-prod": "vue-cli-service build --mode production --target app --dest demo-prod",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jchou24/table-form.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "demo/*"
  ],
  "dependencies": {
    "awesomplete": "^1.1.5",
    "ci": "^1.0.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-mocha": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "animate.css": "^3.7.2",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "jquery": "^3.4.1",
    "lint-staged": "^8.1.5",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.3.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "resize-sensor": "0.0.6",
    "sass-loader": "^8.0.0",
    "vue-clickaway": "^2.2.2",
    "vue-clipboard2": "^0.3.1",
    "vue-context": "^5.0.0",
    "vue-json-pretty": "^1.6.3",
    "vue-multiselect": "^2.1.6",
    "vue-notification": "^1.3.20",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.2",
    "vuesax": "^3.12.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
